@model VistaLibros<h2 style="color:blue">    Registro @Model.Posicion de @ViewData["ULTIMO"]    </h2><div>    <ul class="list-group list-group-horizontal">        <li class="list-group-item list-group-item-warning">            <a asp-controller="Libros" asp-action="PaginarRegistroVistaLibros" asp-route-posicion="1">Primero</a>        </li>        <li class="list-group-item list-group-item-warning">            <a asp-controller="Libros" asp-action="PaginarRegistroVistaLibros" asp-route-posicion="@ViewData["ANTERIOR"]">Anterior</a>        </li>        <li class="list-group-item list-group-item-warning">            <a asp-controller="Libros" asp-action="PaginarRegistroVistaLibros" asp-route-posicion="@ViewData["SIGUIENTE"]">Siguiente</a>        </li>        <li class="list-group-item list-group-item-warning">            <a asp-controller="Libros" asp-action="PaginarRegistroVistaLibros" asp-route-posicion="@ViewData["ULTIMO"]">Último</a>        </li>    </ul></div><div>    <dl class="row">        <dt class="col-sm-2">Titulo</dt>        <dd class="col-sm-10">@Model.Titulo</dd>        <dt class="col-sm-2">Autor</dt>        <dd class="col-sm-10">@Model.Autor</dd>        <dt class="col-sm-2">Portada</dt>        <dd class="col-sm-10">            <img src="~/images/@Model.Portada" style="width:100px; height:150px"   /></dd>    </dl></div>